{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:default",
    "core:app:default",
    "store:default",
    "log:default",
    "os:default",
    "websocket:default",
    "websocket:allow-connect",
    "sentry:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.uz"
        },
        {
          "url": "https://*.uz/services/platon-auth/api/eimzo/challenge"
        },
        {
          "url": "https://*.uz/services/platon-auth/api/eimzo"
        }
      ]
    }
  ]
}
