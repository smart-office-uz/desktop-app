{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:default",
    "core:app:default",
    "log:default",
    "os:default",
    "websocket:default",
    "websocket:allow-connect",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://smart-office.com"
        },
        {
          "url": "https://smart-office.uz/services/platon-auth/api/eimzo/challenge"
        },
        {
          "url": "https://smart-office.uz/services/platon-auth/api/eimzo"
        }
      ]
    }
  ]
}